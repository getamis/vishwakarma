apiVersion: v1
clusters:
- cluster:
    certificate-authority-data: ${cluster_ca}
    server: ${api_server_endpoint}
  name: ${cluster_name}
contexts:
- context:
    cluster: ${cluster_name}
    namespace: default
    user: ${cluster_name}
  name: ${cluster_name}
current-context: ${cluster_name}
kind: Config
preferences: {}
users:
- name: ${cluster_name}
  user:
    exec:
      apiVersion: client.authentication.k8s.io/v1alpha1
      args:
      - token
      - -i
      - ${cluster_name}
      command: aws-iam-authenticator